class Backpack:
    def __init__(self, color, size):
        self.color=color
        self.size=size
        self.items=[]
        self.open=False
    
    def openBag(self):
        self.open=True
        print("Bag Open")
    
    def closeBag(self):
        self.open=False
        print("Bag Closed")
        
    def setLevel(self, newLevel):
        self.level=newLevel
        print(self.level)   
